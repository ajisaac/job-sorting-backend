<div class="search-pane pane">
    <div class="block-title-header">
        <hr>
        <h4 class="expandable">Label Filter</h4>
    </div>
    <div class="expandable-content">
        {{#labels}}
            <div>
                <label>
                    <input type="checkbox"
                           name="{{key}}"
                        {{#value}}checked{{/value}}>
                    {{key}}
                </label>
            </div>
        {{/labels}}
    </div>
</div>